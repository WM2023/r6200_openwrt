#include "mt7621_5.15.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
    compatible = "netgear,r6220", "mediatek,mt7621-soc";
    model = "Netgear R6220";
    
    aliases {
        led-boot = &led_power;
        led-failsafe = &led_power;
        led-running = &led_power;
        led-upgrade = &led_power;
        label-mac-device = &gmac0;
    };

    keys {
        compatible = "gpio-keys";

        wps {
            label = "wps";
            gpios = <&gpio 7 GPIO_ACTIVE_LOW>;
            linux,code = <KEY_WPS_BUTTON>;
        };

        wifi {
            label = "wifi";
            gpios = <&gpio 8 GPIO_ACTIVE_LOW>;
            linux,code = <KEY_RFKILL>;
        };

        reset {
            label = "reset";
            gpios = <&gpio 14 GPIO_ACTIVE_LOW>;
            linux,code = <KEY_RESTART>;
        };
    };

    leds {
        compatible = "gpio-leds";

        led_power: power {
            label = "green:power";
            gpios = <&gpio 18 GPIO_ACTIVE_LOW>;
        };

        usb {
            label = "green:usb";
            gpios = <&gpio 15 GPIO_ACTIVE_LOW>;
            trigger-sources = <&xhci_ehci_port1>, <&ehci_port2>;
            linux,default-trigger = "usbport";
        };

        internet {
            label = "green:wan";
            gpios = <&gpio 13 GPIO_ACTIVE_LOW>;
        };

        wifi {
            label = "green:wifi";
            gpios = <&gpio 16 GPIO_ACTIVE_LOW>;
            linux,default-trigger = "phy0tpt";
        };

        wps {
            label = "green:wps";
            gpios = <&gpio 12 GPIO_ACTIVE_LOW>;
        };
    };

    reg_usb_vbus: regulator {
        compatible = "regulator-fixed";
        regulator-name = "usb_vbus";
        regulator-min-microvolt = <5000000>;
        regulator-max-microvolt = <5000000>;
        gpio = <&gpio 10 GPIO_ACTIVE_HIGH>;
        enable-active-high;
    };
};

&xhci {
    vbus-supply = <&reg_usb_vbus>;
};

&nand {
    status = "okay";

    partitions {
        compatible = "fixed-partitions";
        #address-cells = <1>;
        #size-cells = <1>;

        partition@0 {
           label = "u-boot";
            reg = <0x0 0x100000>;
            sercomm,scpart-id = <0>;
            read-only;
        };

        partition@100000 {
            label = "SC PID";
            reg = <0x100000 0x100000>;
            sercomm,scpart-id = <1>;
            read-only;
        };

        partition@200000 {
            label = "kernel";
            reg = <0x200000 0x400000>;
            sercomm,scpart-id = <2>;
        };

        partition@600000 {
            label = "ubi";
            reg = <0x600000 0x1c00000>;
            sercomm,scpart-id = <3>;
        };

        partition@2200000 {
            label = "English UI";
            reg = <0x2200000 0x200000>;
            sercomm,scpart-id = <4>;
            read-only;
        };

        partition@2400000 {
            label = "ML1";
            reg = <0x2400000 0x200000>;
            sercomm,scpart-id = <5>;
            read-only;
        };

        partition@2600000 {
            label = "ML2";
            reg = <0x2600000 0x200000>;
            sercomm,scpart-id = <6>;
            read-only;
        };

        partition@2800000 {
            label = "ML3";
            reg = <0x2800000 0x200000>;
            sercomm,scpart-id = <7>;
            read-only;
        };

        partition@2a00000 {
            label = "ML4";
            reg = <0x2a00000 0x200000>;
            sercomm,scpart-id = <8>;
            read-only;
        };

        partition@2c00000 {
            label = "ML5";
            reg = <0x2c00000 0x200000>;
            sercomm,scpart-id = <9>;
            read-only;
        };

        factory: partition@2e00000 {
            label = "factory";
            reg = <0x2e00000 0x100000>;
            sercomm,scpart-id = <10>;
            read-only;
        };

        partition@2f00000 {
            label = "SC Private Data";
            reg = <0x2f00000 0x100000>;
            sercomm,scpart-id = <11>;
            read-only;
        };

        partition@3000000 {
            label = "POT";
            reg = <0x3000000 0x200000>;
            sercomm,scpart-id = <12>;
            read-only;
        };

        partition@3200000 {
            label = "Traffic Meter";
            reg = <0x3200000 0x200000>;
            sercomm,scpart-id = <13>;
            read-only;
        };

        partition@3400000 {
            label = "DPF";
            reg = <0x3400000 0x200000>;
            sercomm,scpart-id = <14>;
            read-only;
        };

        partition@3600000 {
            label = "SC Nvram";
            reg = <0x3600000 0x200000>;
            sercomm,scpart-id = <15>;
            read-only;
        };

        partition@3800000 {
            label = "Ralink Nvram";
            reg = <0x3800000 0x200000>;
            sercomm,scpart-id = <16>;
            read-only;
        };

        partition@3a00000 {
            label = "Ralink Reserved";
            reg = <0x3a00000 0x200000>;
            sercomm,scpart-id = <17>;
            read-only;
        };

        partition@3c00000 {
            label = "ML6";
            reg = <0x3c00000 0x200000>;
            sercomm,scpart-id = <18>;
            read-only;
        };

        partition@3e00000 {
            label = "Upgrade Flag";
            reg = <0x3e00000 0x200000>;
            sercomm,scpart-id = <19>;
            read-only;
        };

        partition@4000000 {
            label = "Reserved Block3";
            reg = <0x4000000 0x200000>;
            sercomm,scpart-id = <20>;
            read-only;
        };

        partition@4200000 {
            label = "Reserved Block4";
            reg = <0x4200000 0x3c00000>;
            sercomm,scpart-id = <21>;
            read-only;
        };
    };
};

&factory {
    nvmem-layout {
        compatible = "fixed-layout";
        #address-cells = <1>;
        #size-cells = <1>;

        eeprom_factory_0: eeprom@0 {
            reg = <0x0 0x512>;
        };
 
        macaddr_factory_4: macaddr@4 {
            compatible = "mac-base";
            reg = <0x4 0x6>;
            #nvmem-cell-cells = <1>;
        };
 
        eeprom_factory_8000: eeprom@8000 {
            reg = <0x8000 0x512>;
        };
    };
};

&pcie0 {
    wifi@0,0 { 
        #address-cells = <3>;
        #size-cells = <2>;
        compatible = "mediatek,mt76";
        reg = <0x0 0 0 0 0>;
        ieee80211-freq-limit = <5000000 6000000>;
        nvmem-cells = <&eeprom_factory_8000>, <&macaddr_factory_4 4>;
        nvmem-cell-names = "eeprom", "mac-address";
    };
};

&pcie1 {
    wifi@0,0 { 
        #address-cells = <3>;
        #size-cells = <2>;
        compatible = "mediatek,mt76";
        reg = <0x0 0 0 0 0>;
        ieee80211-freq-limit = <5000000 6000000>;
        nvmem-cells = <&eeprom_factory_8000>, <&macaddr_factory_4 4>;
        nvmem-cell-names = "eeprom", "mac-address";
    };
};

&pcie2 {
    wifi@0,0 {
        #address-cells = <3>;
        #size-cells = <2>;
        compatible = "mediatek,mt76";
       reg = <0x0 0 0 0 0>;
        ieee80211-freq-limit = <2400000 2500000>;
        nvmem-cells = <&eeprom_factory_0>, <&macaddr_factory_4 3>;
        nvmem-cell-names = "eeprom", "mac-address";
    };
};

&state_default {
    gpio {
        groups = "uart3", "uart2", "jtag";
        function = "gpio";
    };
};

&switch0 {
    ports {
        port@0 {
            status = "okay";
            label = "lan4";
            nvmem-cells = <&macaddr_factory_4 8>;
            nvmem-cell-names = "mac-address";
        };

        port@1 {
            status = "okay";
            label = "lan3";
            nvmem-cells = <&macaddr_factory_4 7>;
            nvmem-cell-names = "mac-address";
        };

        port@2 {
            status = "okay";
            label = "lan2";
            nvmem-cells = <&macaddr_factory_4 6>;
            nvmem-cell-names = "mac-address";
        };

        port@3 {
            status = "okay";
            label = "lan1";
            nvmem-cells = <&macaddr_factory_4 5>;
            nvmem-cell-names = "mac-address";
        };
    };
};
